					"browser": true,
					"es6": true,
					"jest": true,
					"node": true
				},
				"extends": "liferay",
				"parser": "babel-eslint",
				"parserOptions": {
					"ecmaVersion": 7
				},
				"plugins": [
					"./node_modules/eslint-plugin-react"
